   "source": [
    "import xlwings as xw\n",
    "\n",
    "\n",
    "\n",
    "def refresh(workbook):\n",
    "\tapp = xw.App(visible=False)\n",
    "\twb=xw.Book(workbook)\n",
    "\twb.api.RefreshAll()\n",
    "\twb.save()\n",
    "\tapp.quit()\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "\tworkbook=\"2KR-PRE-POST-Template.xlsx\"\n",
    "\trefresh(workbook)"
   ]
